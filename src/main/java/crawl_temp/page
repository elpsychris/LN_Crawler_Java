    $(document).ready(function () {
        token = 'INzHhH95fMNrYq6CwgRmpahT9j7fTTe9nugvpcN9'

        $(".mobile-toggle header").click(function(){
            $(this).parent().find(".listall_summary").toggle();
            $(this).parent().find(".sect-body").toggle();                
        });
        $(".navbar-user_toggle").on('click', function(){
            $("#navbar-user").toggle();        
        });       
        
        $(window).on('scroll', function () {
            if ($(window).scrollTop() > 50) {
                $('.backtoTop').show();
            } else {
                $('.backtoTop').hide();
            }
        });
        
        $('.backtoTop').on('click',function(){
            var scrollTo = $(this).data('scrollto');
            $('html, body').animate({scrollTop: $(scrollTo).offset().top});   
        });
        
        $('#mainpart').css({'min-height' : $(window).height() - $('#footer').outerHeight(true)})
        $(window).on('resize',function(){
            $('#mainpart').css({'min-height' : $(window).height() - $('#footer').outerHeight(true)})
        })
        
        $( "#sidenav-icon" ).click(function() {
            $( this ).toggleClass( "active" );
            $( ".navbar-menu" ).toggleClass( "none" );
        });

                $('#navbar').scrollupbar();
        
                var cboxopen = true;

        $('#cboxtoggle').on('click', function() {
            Cookies.set('cboxopen', !cboxopen, { expires: 365 });

            $('#cboxframe').toggleClass('block none');

            var toggletext = $('#cboxtoggle').text();
            $('#cboxtoggle').text(toggletext == 'CHAT' ? 'ĐÓNG' : 'CHAT');
        });
            });
